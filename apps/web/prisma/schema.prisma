datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Token {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name     String
  symbol   String
  decimals Int
  address  String
  logo    String
  chainId Int
}
